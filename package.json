{
  "name": "track24-framework",
  "version": "1.1.3",
  "description": "Track24 Framework",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "css",
    "es",
    "lib",
    "umd"
  ],
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "build": "npm run build-css && nwb build-react-component",
    "clean": "nwb clean-module && nwb clean-demo",
    "start": "nwb serve-react-demo && npm run watch-css",
    "test": "nwb test-react",
    "test:coverage": "nwb test-react --coverage",
    "test:watch": "nwb test-react --server",
    "deploy": "npm run build && cd ../../ && node aws upload modules/framework/ /other/track24-framework/umd",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "storybook-export": "build-storybook -c .storybook -o docs"
  },
  "dependencies": {
    "fg-loadcss": "^2.0.1",
    "node-sass-chokidar": "^1.3.0"
  },
  "peerDependencies": {
    "react": "16.x"
  },
  "devDependencies": {
    "@storybook/addon-knobs": "^3.4.8",
    "@storybook/react": "^3.4.8",
    "aws-sdk": "^2.267.1",
    "bluebird": "^3.5.1",
    "dotenv": "^6.0.0",
    "fs": "0.0.1-security",
    "make-runnable": "^1.3.6",
    "np": "^3.0.4",
    "nwb": "0.21.x",
    "path": "^0.12.7",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1"
  },
  "author": "",
  "homepage": "",
  "license": "MIT",
  "repository": "",
  "keywords": [
    "track24",
    "framework"
  ]
}
